Usuario json
    usuario                 Text
    senha                   Text
    email                   Text

Responsavel json
    nome                    Text
    cpfResponsavel          Text
    celular                 Text
    telefone                Text
    complemento             Text
    numeroCasa              Int
    cep                     Text
    idUsuario               UsuarioId
    sexo                    Text

Aluno json
    nome                    Text
    limite                  Double
    cpfResponsavel          Text
    sexo                    Text
    status                  Text
    idUsuario               UsuarioId
    dataNascimento          Text
    
Categoria json
    nome                    Text
    
Produto json
	nome                    Text
	valor                   Double
	idCategoria             CategoriaId
	
ContaCorrente json 
    idAluno                 AlunoId
    datac                   Text
    tipo 	   	            Int
    saldo                   Text
    
Alergeno json 
    nome                    Text
    
AlergenoProduto json
    prodrgid                ProdutoId
    alergid                 AlergenoId 
    
Compra json
    idAluno		        	AlunoId
    total		        	Double
    dataCompra		        Text	
    
ItemCompra json
    idProduto		        ProdutoId
    quantidade		        Int
    valorItem		        Double	
    
Restricao json
    idAluno                 AlunoId
    idProduto               ProdutoId
    UniqueRestricao         idAluno idProduto
        
AlergenoAluno json
    idAluno                 AlunoId
    idAlergeno              AlergenoId
    UniqueAlergenoAluno     idAluno idAlergeno